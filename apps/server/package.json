{
  "name": "@blastapi/server",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Backend server for BlastAPI",
  "scripts": {
    "dev": "tsx watch --require dotenv/config src/index.ts",
    "build": "tsc",
    "start": "node --require dotenv/config dist/index.js",
    "check-types": "tsc --noEmit",
    "test": "pnpm run test:unit",
    "test:unit": "jest --config jest.config.js",
    "test:unit:watch": "jest --config jest.config.js --watch",
    "test:unit:coverage": "jest --config jest.config.js --coverage",
    "test:unit:coverage:watch": "jest --config jest.config.js --coverage --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write 'src/**/*.{ts,json}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.9.0",
  "dependencies": {
    "@blastapi/db": "workspace:*",
    "@blastapi/utils": "workspace:*",
    "@blastapi/validators": "workspace:*",
    "axios": "^1.9.0",
    "bullmq": "^5.56.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "ioredis": "^5.6.1",
    "redis": "^5.5.5",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
