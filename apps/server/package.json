{
  "name": "@blastapi/server",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Backend server for BlastAPI",
  "scripts": {
    "dev": "tsx watch --require dotenv/config src/index.ts",
    "build": "tsc",
    "start": "node --require dotenv/config dist/index.js",
    "check-types": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write 'src/**/*.{ts,json}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "@blastapi/db": "workspace:*",
    "@blastapi/utils": "workspace:*",
    "@blastapi/validators": "workspace:*",
    "axios": "^1.9.0",
    "bull": "^4.16.5",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.7.0",
    "morgan": "^1.10.1",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "socket.io": "^4.8.1",
    "uuid": "catalog:uuid"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.17.1",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
